<h1>Devices#index</h1>
<p>Find me in app/views/devices/index.html.erb</p>

<ul>
  <% @devices.each do |device| %>
    <li>
      <a href="<%= project_device_path(device) %>">
        <%= device.name %>
      </a>
    </li>
  <% end %>
</ul>

<%= link_to "New Device", new_project_device_path(@project.id) %>
